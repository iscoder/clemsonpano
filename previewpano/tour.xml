		<preview url="panos/aerial_1.tiles/preview.jpg" />

		<image type="CUBE" multires="true" tilesize="512" progressive="false">
			<level tiledimagewidth="2387" tiledimageheight="2387">
				<cube url="panos/aerial_1.tiles/%s/l3/%v/l3_%s_%v_%h.jpg" />
			</level>
			<level tiledimagewidth="1194" tiledimageheight="1194">
				<cube url="panos/aerial_1.tiles/%s/l2/%v/l2_%s_%v_%h.jpg" />
